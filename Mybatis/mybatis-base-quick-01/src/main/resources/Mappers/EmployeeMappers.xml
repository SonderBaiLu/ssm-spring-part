<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace= mapper接口的全限定符 -->
<mapper namespace="com.atqicat.Mapper.EmployeeMapper">
    <!--xml方式写sql语句 没有java代码-->
    <!--声明标签写sql语句 crud select insert update delete
    每一个标签对应接口的一个方法！ 方法的一个实现
    -->
    <select id="queryById" resultType="com.atqicat.pojp.Employee">
        select * from t_emp where empId = #{id}
    </select>
    <delete id="deleteById" >
        delete from t_emp where empId = #{id}
    </delete>
</mapper>